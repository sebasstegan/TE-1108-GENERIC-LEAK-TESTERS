  "trigger": [
    {
      "event": "%s%Diagnostics::UPTIME%/s%",
      "actions": [
        {
          "objectType": "WriteToSymbol",
          "active": true,
          "symbolExpression": "%s%PLC1.GVL_HMI.sUserName%/s%",
          "value": {
            "objectType": "Symbol",
            "valueType": "tchmi:general#/definitions/String",
            "symbolExpression": "%s%GetCurrentUser::name%/s%"
          },
          "asyncWait": true
        },
        {
          "objectType": "WriteToSymbol",
          "active": true,
          "symbolExpression": "%s%PLC1.GVL_HMI.sUserGroupIn%/s%",
          "value": {
            "objectType": "Symbol",
            "valueType": "tchmi:general#/definitions/String",
            "symbolExpression": "%s%GetCurrentUser::groups%/s%"
          },
          "asyncWait": true
        },
        {
          "objectType": "WriteToSymbol",
          "active": true,
          "symbolExpression": "%s%PLC1.GVL_HMI.nMainRegion%/s%",
          "value": {
            "objectType": "Symbol",
            "valueType": "tchmi:general#/definitions/String",
            "symbolExpression": "%i%MainFrame%/i%"
          },
          "asyncWait": true
        },
        {
          "objectType": "Condition",
          "active": true,
          "parts": [
            {
              "if": [
                {
                  "compare1": {
                    "objectType": "Symbol",
                    "valueType": "tchmi:general#/definitions/BOOL",
                    "symbolExpression": "%s%PLC1.GVL_Persistent.stJavasCriptPDF::bMakePDF%/s%"
                  },
                  "compare2": {
                    "objectType": "StaticValue",
                    "valueType": "tchmi:general#/definitions/BOOL",
                    "value": true
                  },
                  "compareOperator": "==",
                  "logic": null
                }
              ],
              "then": [
                {
                  "objectType": "JavaScript",
                  "active": true,
                  "sourceLines": [
                    "var symbol = new TcHmi.Symbol('%s%PLC1.GVL_Persistent.stJavasCriptPDF%/s%');",
                    "    symbol.readEx(function (data) {",
                    "        if (data.error === TcHmi.Errors.NONE) {",
                    "            // Handle result value... ",
                    "            var value = data.value;",
                    "            gerarRelatorioPDF(value);",
                    "            console.log(value);",
                    "        } else {",
                    "            // Handle error... ",
                    "        }",
                    "    });"
                  ],
                  "injectContextObject": false,
                  "asyncWait": true,
                  "waitMode": "Synchronous"
                }
              ]
            },
            {
              "else": []
            }
          ],
          "asyncWait": true
        }
      ]
    }
  ],
